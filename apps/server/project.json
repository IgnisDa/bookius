{
  "root": "apps/server",
  "sourceRoot": "apps/server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "assets": [],
        "outputPath": "dist/apps/server",
        "main": "apps/server/src/main.ts",
        "tsConfig": "apps/server/tsconfig.app.json",
        "tsPlugins": [
          {
            "name": "@nestjs/graphql/plugin",
            "options": {
              "typeFileNameSuffix": [
                ".input.ts",
                ".dto.ts",
                ".result.ts",
                ".interface.ts"
              ],
              "introspectComments": true
            }
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": false,
          "extractLicenses": true,
          "inspect": false,
          "verbose": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "server:build"
      }
    },
    "build-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run model:generate",
          "nx run server:build:production"
        ]
      }
    },
    "start-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run model:migrate-status",
          "nx run model:migrate-deploy",
          "node dist/apps/server/main"
        ]
      }
    }
  },
  "tags": []
}
