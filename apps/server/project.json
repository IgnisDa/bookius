{
  "root": "apps/server",
  "sourceRoot": "apps/server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "assets": [],
        "outputPath": "dist/apps/server",
        "main": "apps/server/src/main.ts",
        "tsConfig": "apps/server/tsconfig.app.json",
        "tsPlugins": [
          {
            "name": "@nestjs/graphql/plugin",
            "options": {
              "typeFileNameSuffix": [
                ".input.ts",
                ".dto.ts",
                ".result.ts",
                ".interface.ts"
              ],
              "introspectComments": true
            }
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": false,
          "extractLicenses": true,
          "inspect": false,
          "verbose": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "server:build"
      }
    },
    "build-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx run model:generate", "nx run server:build:production"]
      }
    },
    "start-prod": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run model:migrate-status",
          "nx run model:migrate-deploy",
          "node dist/apps/server/main"
        ]
      }
    }
  },
  "tags": [],
  "dependencies": {
    "required": [
      "@magic-sdk/admin",
      "@nestjs-modules/mailer",
      "@nestjs/bull",
      "@nestjs/common",
      "@nestjs/core",
      "@nestjs/graphql",
      "@nestjs/mapped-types",
      "@nestjs/passport",
      "@nestjs/platform-express",
      "@nestjs/schedule",
      "@prisma/client",
      "apollo-server-core",
      "apollo-server-errors",
      "apollo-server-express",
      "bull",
      "class-transformer",
      "class-validator",
      "connection-string-parser",
      "graphql",
      "graphql-scalars",
      "graphql-type-json",
      "graphql-upload",
      "helmet",
      "lodash",
      "luxon",
      "magic-sdk",
      "nest-typed-config",
      "nodemailer",
      "passport",
      "passport-headerapikey",
      "prisma",
      "prisma-graphql-type-decimal",
      "reflect-metadata",
      "regenerator-runtime",
      "rxjs",
      "tslib",
      "tslog",
      "unique-names-generator"
    ],
    "development": [
      "@nestjs/schematics",
      "@nestjs/testing",
      "@nrwl/cli",
      "@nrwl/eslint-plugin-nx",
      "@nrwl/jest",
      "@nrwl/linter",
      "@nrwl/nest",
      "@nrwl/node",
      "@nrwl/workspace",
      "@nx-tools/nx-prisma",
      "@types/bull",
      "@types/cron",
      "@types/faker",
      "@types/jest",
      "@types/lodash",
      "@types/luxon",
      "@types/node",
      "@types/nodemailer",
      "@types/passport",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "eslint-config-prettier",
      "jest",
      "prisma-dbml-generator",
      "prisma-nestjs-graphql",
      "ts-jest",
      "ts-node",
      "typescript"
    ]
  }
}
