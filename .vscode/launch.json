{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Nest Debug",
            "runtimeExecutable": "nx",
            "runtimeArgs": [
                "serve",
                "server"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "restart": true,
            "protocol": "auto",
            "port": 9229,
            "autoAttachChildProcesses": true,
        }
    ]
}
